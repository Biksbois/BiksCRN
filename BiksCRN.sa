Initialize {
Specie X=100, Y=1000;
    Sample A {
        CRN {
            Y->2*X, 1;
        }
    }

    Sample B {
        CRN {
            X->2*Y, 1;
        }
    }
}
Protocol {
    Sample A,B = A.Split (0.6, 0.1);
    Equilibrate A for 2 t by 0.0005 each 1 bitesize 100;
    Equilibrate B for 2 t by 0.0005 each 1 bitesize 100;
}